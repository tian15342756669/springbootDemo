<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hispeed.mobile.mapper.UserInfoMapper">


    <insert id="insertUserInfo" parameterType="com.hispeed.mobile.domain.User">
        INSERT OLP_USER_INFO (USER_ID,USER_NAME,USER_AGE,USER_ADDRESS)
        VALUES (#{userId},#{userName},#{userAge},#{userAddress})
    </insert>

</mapper>